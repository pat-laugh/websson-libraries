//sets constructors of function Body
//takes two params: ClassName and BodyParameter

#define HeadParameters typename Head::Parameters
#define HeadPointer typename Head::Pointer
#define HeadEntity typename Head::Entity

ClassName(FunctionHead&& Head, BodyParameter&& Body) : Head(std::move(Head)), Body(std::move(Body)) {}
ClassName(const FunctionHead& Head, const BodyParameter& Body) : Head(Head), Body(Body) {}
ClassName(HeadParameters&& Head, BodyParameter&& Body) : Head(std::move(Head)), Body(std::move(Body)) {}
ClassName(const HeadParameters& Head, const BodyParameter& Body) : Head(Head), Body(Body) {}
ClassName(const HeadPointer& Head, BodyParameter&& Body) : Head(Head), Body(std::move(Body)) {}
ClassName(const HeadPointer& Head, const BodyParameter& Body) : Head(Head), Body(Body) {}
ClassName(const HeadEntity& Head, BodyParameter&& Body) : Head(Head), Body(std::move(Body)) {}
ClassName(const HeadEntity& Head, const BodyParameter& Body) : Head(Head), Body(Body) {}

#undef HeadEntity
#undef HeadPointer
#undef HeadParameters